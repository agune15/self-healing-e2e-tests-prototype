{
  "name": "self-healing-e2e-tests-prototype",
  "version": "1.0.0",
  "description": "TBD",
  "main": "index.js",
  "scripts": {
    "cy:open": "cypress open --e2e",
    "cy:run": "cypress run --browser chrome --spec 'cypress/e2e/*'",
    "reports:combine": "mochawesome-merge reports/mocha-reports/*.json > reports/mocha-reports/combined-report.json",
    "reports:generate": "marge reports/mocha-reports/combined-report.json --reportDir=reports --reportFilename=report --inline",
    "reports:clean": "rm -rf reports/mocha-reports && rm -rf reports/screenshots && rm -rf reports/snapshots",
    "reports": "npm run reports:combine && npm run reports:generate && npm run reports:clean",
    "llm:analyze-failures": "node scripts/aiTestFailureAnalyzer/analyzeTestFailures.js",
    "llm:create-mr": "node scripts/aiTestFailureAnalyzer/createMergeRequestWithFixes.js"
  },
  "keywords": [],
  "author": "Alex Nebot Oller",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^14.4",
    "cypress-mochawesome-reporter": "^3.8.2"
  },
  "type": "module",
  "dependencies": {
    "@google/genai": "^1.7.0",
    "cheerio": "^1.1.0",
    "dotenv": "^17.0.0"
  }
}
